   "source": [
    "class Temp:\n",
    "  __var=10\n",
    "  def getter(self):\n",
    "    return self.__var\n",
    "  def setter(self,newVal):\n",
    "    self.__var=newVal\n",
    "\n",
    "obj=Temp()\n",
    "print(obj.getter())\n",
    "obj.setter(obj.getter()+5)\n",
    "print(obj.getter())"
   ]
